# Unsplash API Configuration
# Get your access key from: https://unsplash.com/developers
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# AWS Cognito Configuration
# Required for authentication features
NEXT_PUBLIC_COGNITO_USER_POOL_ID=your_cognito_user_pool_id
NEXT_PUBLIC_COGNITO_CLIENT_ID=your_cognito_client_id
NEXT_PUBLIC_AWS_REGION=eu-central-1

# Google Analytics Configuration
# Optional: For analytics tracking
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
