{
  "name": "@mostage/studio-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Mostage Studio",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "deploy:dev": "cdk deploy mostage-studio-stack-dev",
    "deploy:prod": "cdk deploy mostage-studio-stack-prod",
    "diff:dev": "cdk diff mostage-studio-stack-dev",
    "diff:prod": "cdk diff mostage-studio-stack-prod",
    "synth:dev": "cdk synth mostage-studio-stack-dev",
    "synth:prod": "cdk synth mostage-studio-stack-prod",
    "destroy:dev": "cdk destroy mostage-studio-stack-dev",
    "destroy:prod": "cdk destroy mostage-studio-stack-prod"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/aws-lambda": "^8.10.130",
    "dotenv": "^16.3.1",
    "typescript": "^5.0.0",
    "source-map-support": "^0.5.21"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0"
  }
}
